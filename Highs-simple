import numpy as np

import matplotlib.pyplot as plt

import highspy

h = highspy.Highs()

#minimize    f  =  x0 +  x1

#subject to              x1 <= 7

#5 <=  x0 + 2x1 <= 15

#6 <= 3x0 + 2x1

#0 <= x0 <= 4; 1 <= x1


x0 = h.addVariable(lb = 0, ub = 4) #lower and upper bounds

x1 = h.addVariable(lb = 1, ub = 7)

h.addConstr(5 <=   x0 + 2*x1 <= 15)

h.addConstr(6 <= 3*x0 + 2*x1)

h.minimize(x0 + x1)
